{% extends ':admin:base.html.twig' %}

{% block body %}


  {{ form_start(form) }}
    <div class="form-group">
      {{ form_label(form.unit) }}
      {{ form_errors(form.unit) }}
      {{ form_widget(form.unit, {'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="form-group">
      {{ form_widget(form.save, {'attr': {'class': 'btn btn-default'}}) }}
    </div>
  {{ form_end(form) }}


{% endblock %}